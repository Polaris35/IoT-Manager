{
  "name": "@iot-manager/proto",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./proto/*": "./proto/*"
  },
  "files": [
    "dist",
    "proto"
  ],
  "scripts": {
    "clean": "shx rm -rf dist src/generated",
    "generate-proto-types": "node generate-proto.js && pnpm run build",
    "build": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "shx": "^0.4.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/microservices": "^11.1.5",
    "google-proto-files": "^5.0.0",
    "protobufjs": "^7.5.4",
    "rxjs": "^7.8.2",
    "ts-proto": "^2.8.3"
  }
}