{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "generate-proto-types": {
      "dependsOn": [],
      "outputs": ["dist/**"],
      "cache": true
    },
    "build": {
      "dependsOn": ["build-libs", "^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "dependsOn": ["generate-proto-types", "build-libs"],
      "cache": false,
      "persistent": true
    },
    "build-libs": {
      "dependsOn": [],
      "outputs": ["dist/**"],
      "cache": true
    }
  }
}
